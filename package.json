{
  "name": "cleaners-app",
  "description": "Cleaners App",
  "version": "0.1.0",
  "authors": [
    "Luam Ramlow <ramlowdev@proton.me>"
  ],
  "private": true,
  "packageManager": "yarn@1.22.17",
  "workspaces": [
    "apps/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ramlow07/Cleaners-app"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
